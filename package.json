{
  "name": "floan",
  "version": "1.0.0",
  "description": "Uncollateralized loans made real",
  "license": "MIT",
  "private": true,
  "scripts": {
    "contract:compile": "yarn workspace @floan/contract compile && node scripts/copyAbi.js",
    "contract:deploy:localhost": "yarn run contract:compile && yarn workspace @floan/contract deploy:localhost",
    "web:start": "yarn workspace @floan/web start",
    "backend:start": "yarn workspace @floan/backend dev"
  },
  "workspaces": [
    "apps/floan-contract",
    "apps/floan-backend",
    "apps/floan-web"
  ],
  "packageManager": "yarn@3.0.2"
}
